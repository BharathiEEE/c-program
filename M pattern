#include <stdio.h>

int main()
{
    int n,mid;
    scanf("%d",&n);
        mid=(n+1)/2;
    for(int i=1;i<=n;i++){
        for(int j=1; j<=n;j++){
        if(j==1||j==n||i==j&&i<=mid||i+j==n+1&&j>=mid)
            printf("*");
        else
            printf(" ");
        }
        printf("\n");}
            
    }



