#include <stdio.h>

int main()
{
    int n,mid;
    scanf("%d",&n);
        mid=(n+1)/2;
    for(int i=1;i<=n;i++){
        for(int j=1; j<=n;j++){
        if(j==mid||i==mid||i==1&&j>=mid||j==1&&i<=mid||i==n&&j<=mid||j==n&&i>=mid)
            printf("*");
        else
            printf(" ");
        }
        printf("\n");}
            
    }



